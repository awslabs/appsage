{
  "AppSage.Core": {
    "IsAppSageWorkspace": true
  },
  "AppSage.Infrastructure.Metric.MetricCollector": {
    "MaxOutputMetricsPerFile": 1000,
    "OutputFilePrefix": "AppSage",
    "AddProviderNameToOutputFile": true
  },

  "AppSage.Infrastructure.Metric.MetricReader": {
    "TakeProviderOutputDataFromLatestRun": true,
    "DocumentMaxParallelism": 20,
    "CacheMetricDataInMemory": true,
    "CacheFilteredMetricDataInMemory": true,
    "FilterMaxParallelism": 20
  },
  "AppSage.MCPServer.Runner": {
    "ListeningPort": 44000
  },

  "AppSage.Query.DynamicCompiler": {
    "SaveQueryAsTemplateGroup": true,
    "TemplateGroupNameForSaving": "DynamicQueryCollector"
  },

  "AppSage.MCPServer.Capability.Tools.ToolDiscovery": {
    "ToolNamesToIgnore": [
      "AppSage_Echo1",
      "AppSage_Echo2"
    ]
  },
  "AppSage.MCPServer.Capabilty.Utility": {
    "MaxBase64EncodedReturnBlobSizeInKB": 256
  },

  "AppSage.Providers.DotNet.SHARED": {
    "MSBuildPath": ""
  },
  "AppSage.Providers.DotNet.Utility": {
    "DocumentMaxParallelism": 10
  },

  "AppSage.Providers.DotNet.BasicCodeAnalysis.DotNetBasicCodeAnalysisProvider": {
    "ProjectMaxParallelism": 10,
    "DocumentMaxParallelism": 10,
    "NamespaceListFileToIdentifyDBRelatedClasses": "C:\\Dev\\SampleAppSageWorkspace\\ProviderConfig\\AppSage.Providers.DotNet.BasicCodeAnalysis.DotNetBasicCodeAnalysisProvider\\DatabaseRelatedNamespaces.txt"
  },

  "AppSage.Providers.DotNet.DependenyAnalysis.DotNetDependencyAnalysisProvider": {
    "SolutionMaxParallelism": 1,
    "ProjectMaxParallelism": 10,
    "DocumentMaxParallelism": 10,
    "LargeMetricDependencyGraphNodeThreshold": 100,
    "LargeMetricDependencyGraphEdgeThreshold": 100,
    "NamespacePrefixToInclude": [ "System.Data", "System.Data.Sql" ],
    "NamespacePrefixToExclude": [ "System", "Microsoft", "Newtonsoft", "NUnit", "Xunit", "Serilog" ]
  },

  "AppSage.Providers.DotNet.AIAnalysis.DotNetAIAnalysisProvider": {
    "ProjectMaxParallelism": 2,
    "FilterImportantDocumentsMaxParallelism": 4,
    "AnalyzeDocumentWithAIMaxParallelism": 10
  },
  "AppSage.Infrastructure.AWS": {
    "CredentialProfileNames": [ "appsage-profile" ]
  },

  "AppSage.Infrastructure.Caching.FileSystemCache": {
    "IgnoreCache": true
  },
  "AppSage.Infrastructure.AI.Bedrock": {
    "MaxConcurrentThreads": 3,
    "ModelId": "amazon.nova-lite-v1:0",
    "Region": "us-east-1",
    "Timeout": 300000,
    "MaxTokens": 4096,
    "Temperature": 0.7,
    "TopK": 40,
    "TopP": 0.9
  },
  "AppSage.Infrastructure.AI.OllamaService": {
    "ModelId": "qwen2.5-coder:1.5b-base",
    "BaseUrl": "http://localhost:11434",
    "Timeout": 180,
    "MaxTokens": 124000,
    "Temperature": 0.3,
    "TopK": 40,
    "TopP": 0.8
  }
}